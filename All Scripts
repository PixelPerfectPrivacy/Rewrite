local found = false
local AfxcaxiRoblox = _G.AFXKey .. ":" .. _G.AFXDiscordID

for _, v in ipairs(keylist) do
    if AfxcaxiRoblox == v then
        found = true
        break
    end
end

if found then
    print("Whitelisting...")
    print("Correct Key and Discord ID")
    print("Done")
    print("You can use this script")
else
    print("Whitelisting...")
    print("Wrong Key or Discord ID")
    print("Done")
    print("You can't use this script")
    setclipboard("https://discord.gg/wRmRgfYQ7A")
    game.Players.LocalPlayer:Kick("The Key or Discord ID is wrong. Please check the script and report to us.")
end

